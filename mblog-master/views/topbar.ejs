<div class="navbar navbar-default navbar-fixed-top" role="navigation">
    <div class="container" >
        <div class="navbar-header" >
            <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a href="/" class="navbar-brand"><%= Sys.cont.siteName %></a>
        </div>

        <nav class="collapse navbar-collapse" role="navigation">
            <ul class="nav navbar-nav">
                <% if (locals.user != null && locals.user.role=="1") { var tmpmenu=Sys.cont.artideType.concat(Sys.cont.adminUrls);}else{tmpmenu=Sys.cont.artideType}%>

                <% tmpmenu.forEach(function(item){ %>
                    <% if (item.child && item.child.length>0){%>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"> <%= item.key%><b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <% item.child.forEach(function(item2){ %>
                                    <li><a href="/<%= item2.value %>"><%=item2.key%></a></li>
                                <%})%>
                            </ul>
                        </li>
                    <%}else{%>
                        <% if(activeTab === item.key){ %>
                                <li class="active">
                                    <a href="/<%= item.value %>"><%= item.key%></a>
                                </li>
                            <% }else{ %>
                                <li>
                                    <a href="/<%= item.value %>"><%= item.key %></a>
                                </li>
                            <% } %>
                        </li>
                    <%}%>
                <% }) %>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <% if (locals.user != null) { %>
                <li>
                    <a href="/user/info/wdwz"><i class="glyphicon glyphicon-user"></i>  <%= locals.user.username %></a>
                </li>
                <li>
                    <a href="/logout"><i class="glyphicon glyphicon-off"></i> 退出</a>
                </li>
                <% }else{ %>
                <li>
                    <!--<a href="#myModal" data-toggle="modal" data-target="#myModal"><i
                                class="glyphicon glyphicon-user"></i> 登录</a>-->
                    <a href="/login"> <i class="glyphicon glyphicon-chevron-up"></i> 登录</a>
                </li>
                <li>
                    <a href="/register"><i class="glyphicon glyphicon-plus"></i> 注册</a>
                </li>
                <% } %>
            </ul>
        </nav>
    </div>
</div>
<div class="custline"></div>